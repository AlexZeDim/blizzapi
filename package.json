{
  "name": "blizzapi",
  "version": "0.0.7",
  "description": "Feature-rich library for Blizzard Battle.net API",
  "homepage": "https://blizzapi.lukem.net",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "Łukasz Wójcik",
    "url": "https://www.lukaszwojcik.net/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/lukemnet/BlizzAPI.git"
  },
  "bugs": {
    "url": "https://github.com/lukemnet/BlizzAPI/issues"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/lukem.net/BlizzAPI/blob/master/LICENSE"
  }],
  "scripts": {
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json"
  },
  "dependencies": {
    "base-64": "0.1.0",
    "cross-fetch": "3.0.1",
    "jsonata": "1.6.4"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.ts"
    ]
  },
  "devDependencies": {
    "@types/base-64": "0.1.2",
    "@types/jest": "23.3.9",
    "@types/jsonata": "1.3.0",
    "@types/node": "11.9.1",
    "jest": "23.6.0",
    "jest-each": "23.6.0",
    "jest-fetch-mock": "2.1.0",
    "prettier": "1.15.2",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.16.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "5.1.2",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.3"
  },
  "keywords": [
    "blizzard",
    "battlenet",
    "battle-net",
    "battle-net-api",
    "api",
    "starcraft2",
    "sc2",
    "wow",
    "worldofwarcraft",
    "world-of-warcraft",
    "d3",
    "diablo3"
