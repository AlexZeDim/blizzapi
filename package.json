    "jest-each": "23.6.0",
    "jest-fetch-mock": "2.1.0",
    "prettier": "1.15.2",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.16.0",
    "tslint-eslint-rules": "5.4.0",
